{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:force": "vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,vue,html}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:prettier": "prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "stylelint --cache -q \"**/*.{vue,css}\"",
    "lint:stylelint:fix": "npm run lint:stylelint -- --fix",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "precommit": "npm lint && npm test && npm build"
  },
  "dependencies": {
    "@quasar/extras": "^1.12.5",
    "axios": "0.24.0",
    "js-cookie": "3.0.1",
    "normalize.css": "8.0.1",
    "pinia": "^2.0.9",
    "qs": "6.10.2",
    "quasar": "^2.5.5",
    "vue": "^3.2.26",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.9",
    "@quasar/vite-plugin": "^1.0.4",
    "@types/jest": "27.4.0",
    "@types/js-cookie": "3.0.1",
    "@types/qs": "6.9.7",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "@vitejs/plugin-vue": "2.0.1",
    "@vue/compiler-sfc": "3.2.26",
    "@vue/test-utils": "2.0.0-rc.16",
    "autoprefixer": "10.4.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-vue": "8.2.0",
    "jest": "27.4.7",
    "jest-serializer-vue": "2.0.2",
    "jest-watch-typeahead": "1.0.0",
    "postcss": "8.4.5",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "rollup-plugin-visualizer": "5.5.2",
    "stylelint": "14.2.0",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended-vue": "1.1.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "3.0.11",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4",
    "vite": "2.7.10",
    "vite-plugin-html-env": "1.0.3",
    "vite-plugin-svg-icons": "1.1.0",
    "vue-eslint-parser": "8.0.1",
    "vue-tsc": "0.30.2",
    "vue3-jest": "27.0.0-alpha.2"
  }
}
